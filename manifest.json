{
    "manifest_version": 3,
    "name": "J & O Codesmith Extension",
    "description": "Productivity extension",
    "version": "1.0.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "tabs"],
    "action": {
        "default_popup": "popup.html"
      },
    "content_scripts": [{
      "js": ["content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }]
  }